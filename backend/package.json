{
    "name": "backend",
    "private": true,
    "type": "module",
    "types": "environment.d.ts",
    "main": "src/index.ts",
    "scripts": {
        "start:prod": "node --require dotenv/config dist/index.js",
        "start:dev": "nodemon --exec \"pnpm run start\"",
        "clean": "rimraf dist",
        "prebuild": "pnpm run clean",
        "build": "tsc --pretty",
        "watch": "tsc -w",
        "start": "pnpm run build && pnpm run start:prod"
    },
    "dependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.28",
        "dotenv": "^16.4.5",
        "express": "^4.18.3",
        "nodemon": "^3.1.0",
        "typescript": "^5.4.2"
    },
    "engines": {
        "node": ">=16.9.0"
    }
}